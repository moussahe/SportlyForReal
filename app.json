{
  "expo": {
    "name": "SportlyApp",
    "slug": "sportly-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sportly.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.sportly.app"
    },
    "plugins": [],
    "extra": {
      "eas": {
        "projectId": "sportly-app"
      }
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "developmentClient": {
      "silentLaunch": true
    },
    "owner": "sportly"
  }
}
